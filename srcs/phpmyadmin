  server {
	listen 8081 default_server;
	listen [::]:8081 default_server;
	root /var/www/html/phpmyadmin;
	index index.php index.htm index.nginx-debian.html;

	server_name _wp;

	location / {
		try_files $uri $uri/ =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/run/php/php7.3-fpm.sock;

	}
}
